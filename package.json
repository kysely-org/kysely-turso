{
  "name": "kysely-turso",
  "version": "0.0.0",
  "description": "Kysely dialects for Turso",
  "type": "module",
  "repository": "git://github.com/kysely-org/kysely-turso.git",
  "homepage": "https://kysely.dev",
  "author": "Igal Klebanov <igalklebanov@gmail.com> (https://github.com/igalklebanov)",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./libsql": {
      "import": {
        "types": "./dist/libsql/index.d.ts",
        "default": "./dist/libsql/index.js"
      },
      "require": {
        "types": "./dist/libsql/index.d.cts",
        "default": "./dist/libsql/index.cjs"
      }
    },
    "./serverless": {
      "import": {
        "types": "./dist/serverless/index.d.ts",
        "default": "./dist/serverless/index.js"
      },
      "require": {
        "types": "./dist/serverless/index.d.cts",
        "default": "./dist/serverless/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "libsql",
    "serverless"
  ],
  "keywords": [
    "kysely",
    "turso",
    "sqlite",
    "libsql",
    "limbo",
    "serverless",
    "dialect",
    "query builder",
    "query",
    "sql",
    "typescript",
    "type-safe",
    "type-safety"
  ],
  "scripts": {
    "biome": "biome",
    "build": "tsup",
    "check:exports": "attw . --pack",
    "check:types": "tsc --noEmit",
    "prepublishOnly": "biome check && pnpm build && pnpm check:exports",
    "test": "run-p up vitest -r",
    "up": "turso dev",
    "vitest": "vitest"
  },
  "peerDependencies": {
    "@libsql/client": "^0.15.14",
    "@tursodatabase/serverless": "^0.1.3",
    "kysely": "^0.28"
  },
  "peerDependenciesMeta": {
    "@libsql/client": {
      "optional": true
    },
    "@tursodatabase/serverless": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.2.2",
    "@libsql/client": "^0.15.14",
    "@tsconfig/node24": "^24.0.1",
    "@tursodatabase/database": "^0.1.4",
    "@tursodatabase/serverless": "^0.1.3",
    "@types/node": "^24.3.0",
    "kysely": "^0.28.5",
    "npm-run-all": "^4.1.5",
    "pathe": "^2.0.3",
    "std-env": "^3.9.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
